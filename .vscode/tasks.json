    "version": "2.0.0",
    "tasks": [
      {
        "label": "pulumi preview",
        "type": "shell",
        "command": "pulumi preview",
        "problemMatcher": ["$tsc"],
        "group": {
          "kind": "test",
          "isDefault": true
        }
      }
      {
        "label": "pulumi up",
        "type": "shell",
        "command": "pulumi up",
        "problemMatcher": ["$tsc"],
        "group": {
          "kind": "build",
          "isDefault": true
        }
      }
    ]
}